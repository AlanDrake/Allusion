{
  "name": "visuallibrary",
  "version": "1.0.0",
  "description": "A tool to manage your visual library",
  "main": "main.bundle.js",
  "scripts": {
    "development": "rimraf dist && webpack --watch --config ./webpack.dev.js --progress --colors",
    "production": "rimraf dist && webpack --config ./webpack.prod.js --progress --colors && ncp ./package.json dist/package.json",
    "start": "electron ./dist/main.bundle.js",
    "package": "rimraf release && npm run production && electron-packager ./dist VisualLibrary --out=./release --asar",
    "lint": "tslint -c tslint.json -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RvanderLaan/VisualLibrary.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RvanderLaan/VisualLibrary/issues"
  },
  "homepage": "https://github.com/RvanderLaan/VisualLibrary#readme",
  "devDependencies": {
    "@types/fs-extra": "^5.0.4",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/uuid": "^3.4.4",
    "css-loader": "^2.1.0",
    "electron": "^4.0.0",
    "electron-packager": "^13.0.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.11.0",
    "prettier": "^1.15.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.2",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.0"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.12.0",
    "@blueprintjs/select": "^3.6.0",
    "fs-extra": "^7.0.1",
    "idb": "^3.0.2",
    "mobx": "^5.9.0",
    "mobx-react-lite": "^0.3.6",
    "react": "^16.8.0-alpha.1",
    "react-dnd": "^7.0.2",
    "react-dnd-html5-backend": "^7.0.2",
    "react-dom": "^16.8.0-alpha.1",
    "styled-components": "^4.1.3",
    "uuid": "^3.3.2"
  }
}
